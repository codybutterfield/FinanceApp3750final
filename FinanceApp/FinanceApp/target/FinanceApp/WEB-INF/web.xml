<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
   <servlet-name>AddFundsServlet</servlet-name>
   <servlet-class>FinanceApp.AddFundsServlet</servlet-class>
  </servlet>
  
  <servlet> 
   <servlet-name>InvestServlet</servlet-name>
	<servlet-class>FinanceApp.InvestServlet</servlet-class>
</servlet>

<servlet-mapping>
   <servlet-name>AddFundsServlet</servlet-name>
   <url-pattern>/addFunds</url-pattern>
</servlet-mapping>
   
<servlet-mapping>
   <servlet-name>InvestServlet</servlet-name>
	<url-pattern>/testResult</url-pattern>
</servlet-mapping>


</web-app>